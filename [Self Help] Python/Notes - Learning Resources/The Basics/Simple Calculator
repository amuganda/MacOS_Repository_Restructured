
# coding: utf-8

# In[ ]:


output = 0
n1 = ""
operation = ""
n2 = ""
n1 = input("Input 1st Number\n")
operator = input("Select Operator?\n")
n2 = input("Input 2nd Number\n")

floatnum1 = float(n1)
floatnum2 = float(n2)

if operator == "*":
    output=floatnum1*floatnum2
if operator == "/":
    output=floatnum1/floatnum2
if operator == "+":
    output=floatnum1+floatnum2
if operator == "-":
    output=floatnum1-floatnum2


print("Answer: "+str(output))

